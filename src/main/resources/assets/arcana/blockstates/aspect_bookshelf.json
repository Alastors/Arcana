{
  "multipart": [
      {   "when": { "facing": "north" },
          "apply": { "model": "arcana:block/aspect_bookshelf" }
      },
      {   "when": { "facing": "east" },
          "apply": { "model": "arcana:block/aspect_bookshelf", "y": 90 }
      },
      {   "when": { "facing": "south" },
          "apply": { "model": "arcana:block/aspect_bookshelf", "y": 180 }
      },
      {   "when": { "facing": "west" },
          "apply": { "model": "arcana:block/aspect_bookshelf", "y": 270 }
      },
      {   "when": { "level": "1","facing": "north" },
          "apply": { "model": "arcana:block/aspect_bookshelf_vial" }
      },
      {   "when": { "level": "1","facing": "east" },
          "apply": { "model": "arcana:block/aspect_bookshelf_vial", "y": 90 }
      },
      {   "when": { "level": "1","facing": "south" },
          "apply": { "model": "arcana:block/aspect_bookshelf_vial", "y": 180 }
      },
      {   "when": { "level": "1","facing": "west" },
          "apply": { "model": "arcana:block/aspect_bookshelf_vial", "y": 270 }
      }
  ]
}